<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
        jcr:language="de"
        jcr:mixinTypes="[mix:language]"
        jcr:primaryType="sling:Folder"
        sling:basename="composum-assets">

    <_list jcr:primaryType="sling:MessageEntry"
            sling:key="List" sling:message="Liste"/>
    <large jcr:primaryType="sling:MessageEntry"
            sling:key="large" sling:message="groß"/>
    <small jcr:primaryType="sling:MessageEntry"
            sling:key="small" sling:message="klein"/>
    <go-up jcr:primaryType="sling:MessageEntry"
            sling:key="Go Up" sling:message="nach Oben"/>

    <image jcr:primaryType="sling:MessageEntry"
            sling:key="Image" sling:message="Bild"/>
    <configuration jcr:primaryType="sling:MessageEntry"
            sling:key="Configuration" sling:message="Konfiguration"/>

    <cancel jcr:primaryType="sling:MessageEntry"
            sling:key="Cancel" sling:message="Abbrechen"/>
    <save jcr:primaryType="sling:MessageEntry"
            sling:key="Save" sling:message="Speichern"/>
    <upload jcr:primaryType="sling:MessageEntry"
            sling:key="Upload" sling:message="Hochladen"/>
    <delete jcr:primaryType="sling:MessageEntry"
            sling:key="Delete" sling:message="Löschen"/>
    <remove jcr:primaryType="sling:MessageEntry"
            sling:key="Remove" sling:message="Entfernen"/>
    <transform jcr:primaryType="sling:MessageEntry"
            sling:key="Transform" sling:message="Umwandeln"/>
    <reload jcr:primaryType="sling:MessageEntry"
            sling:key="Reload" sling:message="Neu Laden"/>
    <drill-down jcr:primaryType="sling:MessageEntry"
            sling:key="Drill Down" sling:message="Drill-Down"/>

    <delete-image jcr:primaryType="sling:MessageEntry"
            sling:key="Delete Image" sling:message="Bild löschen"/>

    <create-asset jcr:primaryType="sling:MessageEntry"
            sling:key="Create a New Asset" sling:message="Ein neues Asset erzeugen"/>
    <delete-asset jcr:primaryType="sling:MessageEntry"
            sling:key="Delete Asset" sling:message="Asset löschen"/>
    <upload-original jcr:primaryType="sling:MessageEntry"
            sling:key="Upload Asset Original" sling:message="Ein neues Original hochladen"/>

    <create-asset-configuration jcr:primaryType="sling:MessageEntry"
            sling:key="Create Asset Configuration" sling:message="Konfiguration anlegen"/>
    <to-image-asset jcr:primaryType="sling:MessageEntry"
            sling:key="Transform into an Image Asset" sling:message="Umwandeln in ein Bild-Asset"/>
    <to-simple-image jcr:primaryType="sling:MessageEntry"
            sling:key="Transform into a simple Image" sling:message="Umwandeln in ein einfaches Bild"/>
    <asset-originals jcr:primaryType="sling:MessageEntry"
            sling:key="Asset Files (Originals)" sling:message="Asset-Dateien (Originale)"/>
    <renditions-view jcr:primaryType="sling:MessageEntry"
            sling:key="Renditions View" sling:message="Renditions-Ansicht"/>

    <filter jcr:primaryType="nt:unstructured">

        <all jcr:primaryType="sling:MessageEntry"
                sling:key="All" sling:message="Alles"/>
        <all-hint jcr:primaryType="sling:MessageEntry"
                sling:key="show all available asset object types" sling:message="alle Asset-Objekt-Typen anzeigen"/>
        <image-asset jcr:primaryType="sling:MessageEntry"
                sling:key="Image Asset" sling:message="Bild-Asset"/>
        <image-asset-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to 'Composum Assets' image objects"
                sling:message="Beschränkung auf 'Composum Assets' Bild-Objekte"/>
        <image-simple jcr:primaryType="sling:MessageEntry"
                sling:key="Simple Image" sling:message="einfaches Bild"/>
        <image-simple-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to simple image files"
                sling:message="Beschränkung auf einfache Bilder"/>
        <image jcr:primaryType="sling:MessageEntry"
                sling:key="Image" sling:message="Bild"/>
        <image-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to image asset or image file resources"
                sling:message="Beschränkung auf Bild-Assets oder Bild-Dateien"/>
        <video-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to video file objects"
                sling:message="Beschränkung auf Video-Dateien"/>
        <audio-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to audio file objects"
                sling:message="Beschränkung auf Audio-Dateien"/>
        <document jcr:primaryType="sling:MessageEntry"
                sling:key="Document" sling:message="Dokument"/>
        <document-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to document file objects"
                sling:message="Beschränkung auf Dokument-Dateien"/>
        <binary-hint jcr:primaryType="sling:MessageEntry"
                sling:key="restrict to binary file objects"
                sling:message="Beschränkung auf Daten-Dateien"/>

    </filter>

    <navigator jcr:primaryType="nt:unstructured">

        <tree jcr:primaryType="sling:MessageEntry"
                sling:key="Tree" sling:message="Baum"/>
        <browse jcr:primaryType="sling:MessageEntry"
                sling:key="Browse" sling:message="Browsen"/>
        <search jcr:primaryType="sling:MessageEntry"
                sling:key="Search" sling:message="Suche"/>

        <go-one-level-up jcr:primaryType="sling:MessageEntry"
                sling:key="Go one level Up" sling:message="Eine Ebene nach Oben"/>
        <small-thumbnails jcr:primaryType="sling:MessageEntry"
                sling:key="Small Thumbnails" sling:message="Kleine Miniaturen"/>
        <large-thumbnails jcr:primaryType="sling:MessageEntry"
                sling:key="Large Thumbnails" sling:message="Große Miniaturen"/>
        <asset-list jcr:primaryType="sling:MessageEntry"
                sling:key="Assets List" sling:message="Asset-Liste"/>
        <open-containing-folder jcr:primaryType="sling:MessageEntry"
                sling:key="Open containing Folder" sling:message="Ordner zum Bild öffnen"/>

        <search-root jcr:primaryType="sling:MessageEntry"
                sling:key="Search Root" sling:message="Such-Basis"/>
        <select-root-folder jcr:primaryType="sling:MessageEntry"
                sling:key="select root folder" sling:message="Basis-Folder auswählen"/>
        <folder-as-root jcr:primaryType="sling:MessageEntry"
                sling:key="use current folder as root" sling:message="aktuellen Folder als Basis nutzen"/>
    </navigator>

    <config jcr:primaryType="nt:unstructured">

        <SelectLayer jcr:primaryType="sling:MessageEntry"
                sling:key="Select Layer" sling:message="Ebene Wählen"/>
        <Base jcr:primaryType="sling:MessageEntry"
                sling:key="Base" sling:message="Basis"/>
        <Default jcr:primaryType="sling:MessageEntry"
                sling:key="Default" sling:message="Standard"/>
        <Extension jcr:primaryType="sling:MessageEntry"
                sling:key="Extension" sling:message="Erweiterung"/>
        <Category jcr:primaryType="sling:MessageEntry"
                sling:key="Category" sling:message="Kategorie"/>
        <ExampleImage jcr:primaryType="sling:MessageEntry"
                sling:key="Example Image" sling:message="Beispielbild"/>
        <Crop jcr:primaryType="sling:MessageEntry"
                sling:key="Crop" sling:message="Schnitt"/>
        <Width jcr:primaryType="sling:MessageEntry"
                sling:key="Width" sling:message="Breite"/>
        <Height jcr:primaryType="sling:MessageEntry"
                sling:key="Height" sling:message="Höhe"/>
        <AspectRatio jcr:primaryType="sling:MessageEntry"
                sling:key="Aspect Ratio" sling:message="Seitenverhältnis"/>
        <Vertical jcr:primaryType="sling:MessageEntry"
                sling:key="Vertical" sling:message="Vertikal"/>
        <Scale jcr:primaryType="sling:MessageEntry"
                sling:key="Scale" sling:message="Skalierung"/>
        <Watermark jcr:primaryType="sling:MessageEntry"
                sling:key="Watermark" sling:message="Wasserzeichen"/>
        <Color jcr:primaryType="sling:MessageEntry"
                sling:key="Color" sling:message="Farbe"/>
        <Opacity jcr:primaryType="sling:MessageEntry"
                sling:key="Opacity" sling:message="Deckkraft"/>
        <Font jcr:primaryType="sling:MessageEntry"
                sling:key="Font" sling:message="Zeichensatz"/>
        <Family jcr:primaryType="sling:MessageEntry"
                sling:key="Family" sling:message="Familie"/>
        <Style jcr:primaryType="sling:MessageEntry"
                sling:key="Style" sling:message="Stil"/>
        <Italic jcr:primaryType="sling:MessageEntry"
                sling:key="Italic" sling:message="Kursiv"/>
        <Bold jcr:primaryType="sling:MessageEntry"
                sling:key="Bold" sling:message="Fett"/>
        <Other jcr:primaryType="sling:MessageEntry"
                sling:key="Other" sling:message="Weiteres"/>
        <BlurFactor jcr:primaryType="sling:MessageEntry"
                sling:key="Blur Factor" sling:message="Unschärfe-Faktor"/>
    </config>

</jcr:root>
